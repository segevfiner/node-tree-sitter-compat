{
  "name": "tree-sitter-compat",
  "version": "0.1.5",
  "description": "Compatibility module for old v8 API based language bindings for node-tree-sitter",
  "type": "commonjs",
  "types": "./index.d.ts",
  "main": "./index.js",
  "exports": {
    "types": "./index.d.ts",
    "default": "./index.js"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "build": "node-gyp build",
    "build:prebuild": "prebuild -t 18.20.7 -t 20.19.5 -t 22.21.1 -t 24.11.0 --strip",
    "rebuild": "node-gyp rebuild",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/segevfiner/node-tree-sitter-compat.git"
  },
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter"
  ],
  "author": "Segev Finer <segev208@gmail.com>",
  "license": "MIT",
  "node-gyp": true,
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8",
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^8.5.0",
    "prebuild-install": "^7.1.3"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.6",
    "@types/node": "^20.19.23",
    "prebuild": "^13.0.1",
    "tree-sitter": "^0.21.1",
    "tree-sitter-c": "0.21.0",
    "tree-sitter-javascript": "0.20.4",
    "vitest": "^3.2.4"
  },
  "files": [
    "binding.gyp",
    "index.js",
    "index.d.ts",
    "src/**"
  ]
}
