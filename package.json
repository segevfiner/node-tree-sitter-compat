{
  "name": "tree-sitter-compat",
  "version": "0.1.2",
  "description": "Compatibility module for old v8 API based language bindings for node-tree-sitter",
  "types": "./index.d.ts",
  "main": "./index.js",
  "exports": {
    "types": "./index.d.ts",
    "default": "./index.js"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "build": "node-gyp build",
    "build:prebuild": "prebuild -t 18.20.7 -t 20.19.0 -t 22.14.0 --strip",
    "rebuild": "node-gyp rebuild",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/segevfiner/node-tree-sitter-compat.git"
  },
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter"
  ],
  "author": "Segev Finer <segev208@gmail.com>",
  "license": "MIT",
  "node-gyp": true,
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^8.3.1",
    "prebuild-install": "^7.1.3"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.5",
    "@types/node": "^20.17.46",
    "prebuild": "^13.0.1",
    "tree-sitter": "^0.21.1",
    "tree-sitter-c": "0.21.0",
    "tree-sitter-javascript": "0.20.4",
    "vitest": "^3.1.3"
  },
  "files": [
    "binding.gyp",
    "index.js",
    "index.d.ts",
    "src/**"
  ]
}
