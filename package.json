{
  "name": "tree-sitter-compat",
  "version": "0.1.2",
  "description": "Compatibility module for old v8 API based language bindings for node-tree-sitter",
  "types": "./index.d.ts",
  "main": "./index.js",
  "exports": {
    "types": "./index.d.ts",
    "default": "./index.js"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "build": "node-gyp build",
    "build:prebuild": "prebuild -t 18.20.7 -t 20.19.0 -t 22.14.0 --strip",
    "rebuild": "node-gyp rebuild",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/segevfiner/node-tree-sitter-compat.git"
  },
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter"
  ],
  "author": "Segev Finer <segev208@gmail.com>",
  "license": "MIT",
  "node-gyp": true,
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^8.4.0",
    "prebuild-install": "^7.1.3"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.5",
    "@types/node": "^20.19.0",
    "prebuild": "^13.0.1",
    "tree-sitter": "^0.21.1",
    "tree-sitter-c": "0.21.0",
    "tree-sitter-javascript": "0.20.4",
    "vitest": "^3.2.3"
  },
  "files": [
    "binding.gyp",
    "index.js",
    "index.d.ts",
    "src/**"
  ]
}
