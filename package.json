{
  "name": "tree-sitter-compat",
  "version": "0.1.3",
  "description": "Compatibility module for old v8 API based language bindings for node-tree-sitter",
  "type": "commonjs",
  "types": "./index.d.ts",
  "main": "./index.js",
  "exports": {
    "types": "./index.d.ts",
    "default": "./index.js"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "build": "node-gyp build",
    "build:prebuild": "prebuild -t 18.20.7 -t 20.19.0 -t 22.14.0 --strip",
    "rebuild": "node-gyp rebuild",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/segevfiner/node-tree-sitter-compat.git"
  },
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter"
  ],
  "author": "Segev Finer <segev208@gmail.com>",
  "license": "MIT",
  "node-gyp": true,
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^8.5.0",
    "prebuild-install": "^7.1.3"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.6",
    "@types/node": "^20.19.13",
    "prebuild": "^13.0.1",
    "tree-sitter": "^0.21.1",
    "tree-sitter-c": "0.21.0",
    "tree-sitter-javascript": "0.20.4",
    "vitest": "^3.2.4"
  },
  "files": [
    "binding.gyp",
    "index.js",
    "index.d.ts",
    "src/**"
  ]
}
