{
  "name": "tree-sitter-compat",
  "version": "0.1.0",
  "description": "Compatibility module for old v8 API based language bindings for node-tree-sitter",
  "types": "./index.d.ts",
  "main": "./index.js",
  "exports": {
    "types": "./index.d.ts",
    "default": "./index.js"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "build": "node-gyp build",
    "build:prebuild": "prebuild -t 18.20.2 -t 20.12.2 --strip",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/segevfiner/node-tree-sitter-compat.git"
  },
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter"
  ],
  "author": "Segev Finer <segev208@gmail.com>",
  "license": "MIT",
  "node-gyp": true,
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^8.0.0",
    "prebuild-install": "^7.1.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.7",
    "prebuild": "^13.0.0",
    "tree-sitter": "^0.21.1",
    "tree-sitter-c": "0.21.0",
    "tree-sitter-javascript": "0.20.4",
    "vitest": "^2.0.3"
  },
  "files": [
    "binding.gyp",
    "index.js",
    "index.d.ts",
    "src/**"
  ]
}
